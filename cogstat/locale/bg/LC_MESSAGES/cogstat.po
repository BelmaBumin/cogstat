#: .././cogstat_stat.py:291 .././cogstat_data.py:392
#, python-format
msgid "Mean: %0.*f"
msgstr "Средна аритметична: %0.*f"

#: .././cogstat_stat.py:292
#, python-format
msgid "Standard deviation: %0.*f"
msgstr "Стандартно отклонение: %0.*f"

#: .././cogstat_stat.py:293
#, python-format
msgid "Skewness: %0.*f"
